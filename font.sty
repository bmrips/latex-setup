\RequirePackage{f1rstlady/prelude}

% Set Libertine as roman font, Biolinum as sans font, TX as math font and Jetbrains Mono as mono
% font if available.  If it is unavailable, use Sourcecode Pro.
\LoadPackage[mono=false]{libertine}
\IfFontExistsTF{Jetbrains Mono}
    {\LoadPackage{fontspec}
     \setmonofont[Scale=MatchLowercase]{Jetbrains Mono}}
    {\renewcommand*{\ttdefault}{lmtt}}
\AtEndPreamble{ % Defer loading of newtxmath as it collides with amssymb and amsthm
    \LoadPackage[libertine,vvarbb]{newtxmath}}
