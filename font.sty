\RequirePackage{f1rstlady/prelude}

% Defer loading of newtxmath as it collides with amssymb and amsthm
\AtEndPreamble{

% Note that the order in which packages are loaded matters.
% See https://github.com/borisveytsman/acmart/issues/402
% and https://github.com/borisveytsman/acmart/issues/410

% TX as math font.
\LoadPackage[libertine]{newtxmath}

% Libertine as roman font, Biolinum as sans font.
\LoadPackage[mono=false]{libertine}

% Inconsolata as monospaced font.
\LoadPackage{fontspec}
\setmonofont[StylisticSet=3]{inconsolata}

} % \AtEndPreamble
